{
  "name": "@aliceo2/run-condition-table",
  "version": "0.1.9",
  "description": "Run Condition Table",
  "main": "main.js",
  "author": "ALICEO2",
  "keywords": [
    "CERN",
    "ALICEO2"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliceO2Group/RunConditionTable.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint app --ext .js --format html --output-file ./reports/static/static-analysis.html",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:show": "run-script-os",
    "eslint:show:linux": "npm run eslint; firefox ./reports/static/static-analysis.html",
    "eslint:show:macos": "npm run eslint; open -a firefox ./reports/static/static-analysis.html",
    "static": "npm run eslint",
    "coverage:test": "mocha --exit test/*",
    "coverage:report": "nyc report --report=html --report=json",
    "coverage": "nyc npm run coverage:test && npm run coverage:report ",
    "start:test": "mkdir -pv ./reports; npm run static; npm run coverage; if [ \"$RUNNING_ENV\" = \"DOCKER\" ]; then chmod -R o+w ./reports; fi;",
    "test:mocha": "env-cmd -f .env.test mocha",
    "reports:show": "open ./reports/**/*.html",
    "reports:clean": "rm -rf ./reports; rm -rf .nyc_output",
    "start:dev": "nodemon app/main.js --watch main.js --watch app --ignore app/public",
    "start:dev:local": "(export RCT_DB_HOST=${RCT_DB_HOST:-localhost}; bash -c 'set -o allexport && ls && source ./docker/dev.env && set +o allexport && npm run start:dev')",
    "deploy:db:local": "./database/setup-db.sh --env ./docker/dev.env",
    "dev": "./rctmake prune,db:export,run,app:attach,stop --target dev",
    "node-dev-env": "(export RCT_DB_HOST=$(docker inspect o2-rct_database -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}'); bash -c 'set -o allexport && ls && source ./docker/dev.env && set +o allexport && node')",
    "dev:up": "./rctmake run[up],app:attach --target dev",
    "dev:up-re": "./rctmake run[up],dump:restore,app:attach --target dev -F ",
    "test": "./rctmake prune,run,prune --target test",
    "prune": "./rctmake prune",
    "app:attach": "docker app:attach o2-rct_application",
    "app:pattach": "docker exec -it o2-rct_application bash",
    "db-ip": "echo $(docker inspect o2-rct_database -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}')",
    "db:check": "./rctmake prune,db:export,run,follow -t dev -S o2-rct_database ",
    "db:attach": "if [ \"$RUNNING_ENV\" != 'DOCKER' ]; then docker exec -it o2-rct_database psql -U postgres -d \"rct-db\"; else export PGPASSWORD=postgres; psql -h o2-rct_database -U postgres -d \"rct-db\"; fi;",
    "db:pattach": "docker exec -it o2-rct_database /bin/bash",
    "db:clean": "./rctmake db:clean",
    "dump:make": "./rctmake dump:make -F ",
    "dump:list": "./rctmake dump:list",
    "dump:restore": "./rctmake dump:restore -F ",
    "dump:remove": "./rctmake dump:remove -F ",
    "tmp": "nodemon ./tmp/tmp.js"
  },
  "bugs": {
    "url": "https://github.com/AliceO2Group/RunConditionTable/issues"
  },
  "homepage": "https://github.com/AliceO2Group/RunConditionTable#readme",
  "dependencies": {
    "@aliceo2/web-ui": "^2.0.0",
    "csvtojson": "^2.0.10",
    "esm": "^3.2.25",
    "joi": "^17.9.2",
    "less": "^4.1.3",
    "pg": "^8.8.0",
    "sequelize": "^6.32.1",
    "supertest": "^6.3.3",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "env-cmd": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-output": "^3.0.1",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "run-script-os": "^1.1.6",
    "sequelize-cli": "^6.6.1",
    "sinon": "^14.0.1",
    "socks-proxy-agent": "^7.0.0"
  },
  "files": [
    "app/",
    "docs/",
    "lib/",
    "database/"
  ],
  "bundleDependencies": [
    "@aliceo2/web-ui",
    "esm",
    "joi",
    "less",
    "pg",
    "sequelize",
    "umzug"
  ]
}
